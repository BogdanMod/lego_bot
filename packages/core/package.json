{
  "name": "@dialogue-constructor/core",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "node -e \"process.platform === 'win32' ? require('child_process').execSync('build.cmd', {stdio: 'inherit'}) : require('child_process').execSync('bash build.sh', {stdio: 'inherit'})\"",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test-db": "tsx src/test-db.ts"
  },
  "dependencies": {
    "@dialogue-constructor/shared": "workspace:*",
    "@vercel/functions": "^1.4.1",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "pino": "^8.17.0",
    "pino-http": "^9.0.0",
    "pino-roll": "^4.0.0",
    "pg": "^8.11.3",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.10",
    "@sentry/node": "^8.0.0",
    "telegraf": "^4.14.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@vercel/node": "^5.5.23",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "overrides": {
    "isomorphic-dompurify": false,
    "jsdom": false,
    "html-encoding-sniffer": "4.0.0"
  }
