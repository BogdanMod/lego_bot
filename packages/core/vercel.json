{
  "buildCommand": "cd ../.. && npm run build --filter=@dialogue-constructor/shared...@dialogue-constructor/core",
  "installCommand": "cd ../.. && npm install",
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

